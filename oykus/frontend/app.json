{
  "expo": {
    "name": "Oykus",
    "slug": "oykus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/img/favicon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./app/assets/img/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#161B28"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/img/favicon.png",
        "backgroundColor": "#161B28"
      }
    },
    "web": {
      "favicon": "./app/assets/img/favicon.png"
    },
    "plugins": [
      [
        "expo-font", {
          "fonts": [
            "node_modules/@expo-google-fonts/nunito/Nunito_400Regular.ttf",
            "node_modules/@expo-google-fonts/nunito/Nunito_700Bold.ttf"
          ]
        }
      ]
    ]
  }
}
